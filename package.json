{
  "name": "iwo-examples",
  "sideEffects": false,
  "version": "0.0.1",
  "description": "iwo examples",
  "main": "dist/index.html",
  "scripts": {
    "prebuild": "rimraf dist",
    "watch": "webpack --config webpack.build.js --progress --colors --watch --mode=development",
    "build": "webpack --config webpack.build.js --progress --colors --mode=development",
    "rollup": "rollup -c rollup.config.ts",
    "start": "start tsc --watch && start es-dev-server --open /docs/"
  },
  "author": "caboyd",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/caboyd/iwo-examples/"
  },
  "engines": {
    "node": ">12.0.0"
  },
  "prettier": {
    "tabWidth": 4,
    "useTabs": false,
    "printWidth": 120,
    "parser": "typescript",
    "semi": true,
    "trailingComma": "es5"
  },
  "dependencies": {
    "gl-matrix": "^3.3.0"
  },
  "devDependencies": {
    "@open-wc/rollup-plugin-html": "^1.1.0",
    "@rollup/plugin-node-resolve": "^8.4.0",
    "@types/node": "^13.13.12",
    "es-dev-server": "^1.56.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "fs-extra": "^9.0.1",
    "prettier": "^1.19.1",
    "rollup": "^2.23.0",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-esm-import-to-url": "^2.1.0",
    "rollup-plugin-glslify": "^1.2.0",
    "rollup-plugin-string": "^3.0.0",
    "rollup-plugin-typescript2": "^0.27.1",
    "typescript": "^3.9.7"
  }
}
