{
  "name": "iwo-examples",
  "sideEffects": false,
  "version": "0.0.1",
  "description": "iwo examples",
  "main": "dist/index.html",
  "scripts": {
    "prebuild": "rimraf docs",
    "build": "rollup -c rollup.config.ts",
    "start": "start rollup -w -c rollup.config.ts && start es-dev-server --open /docs/index.html"
  },
  "author": "caboyd",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/caboyd/iwo-examples/"
  },
  "engines": {
    "node": ">12.0.0"
  },
  "prettier": {
    "tabWidth": 4,
    "useTabs": false,
    "printWidth": 120,
    "parser": "typescript",
    "semi": true,
    "trailingComma": "es5"
  },
  "dependencies": {
    "gl-matrix": "^3.3.0"
  },
  "devDependencies": {
    "@rollup/plugin-html": "^0.2.0",
    "@rollup/plugin-node-resolve": "^8.4.0",
    "@types/node": "^13.13.15",
    "@typescript-eslint/eslint-plugin": "^3.7.1",
    "@typescript-eslint/parser": "^3.7.1",
    "es-dev-server": "^1.57.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "fs-extra": "^9.0.1",
    "prettier": "^1.19.1",
    "rollup": "^2.23.0",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-esm-import-to-url": "^2.1.0",
    "rollup-plugin-glslify": "^1.2.0",
    "rollup-plugin-sourcemaps": "^0.6.2",
    "rollup-plugin-typescript2": "^0.27.1",
    "typescript": "^3.9.7"
  }
}
