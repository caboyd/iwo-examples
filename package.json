{
  "name": "iwo-examples",
  "sideEffects": false,
  "version": "0.0.1",
  "description": "iwo examples",
  "main": "docs/index.html,",
  "scripts": {
    "clean": "rimraf docs",
    "build": "rollup -c rollup.config.ts",
    "watch": "rollup -w -c rollup.config.ts",
    "dev": "start rollup -w -c rollup.config.ts && start es-dev-server --open /docs/index.html",
    "gh-publish": "npm run build&&cd dist&&git add --all&&git commit -m \"Deploy to gh-pages\"&&git push origin gh-pages&&cd.."
  },
  "author": "caboyd",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/caboyd/iwo-examples/"
  },
  "engines": {
    "node": ">12.0.0"
  },
  "prettier": {
    "tabWidth": 4,
    "useTabs": false,
    "printWidth": 120,
    "parser": "typescript",
    "semi": true,
    "trailingComma": "es5",
    "endOfLine": "auto"
  },
  "dependencies": {
    "gl-matrix": "^3.4.3"
  },
  "devDependencies": {
    "@rollup/plugin-html": "^0.2.4",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@types/node": "^18.11.9",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "es-dev-server": "^2.1.0",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "fs-extra": "^10.1.0",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.79.1",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-esm-import-to-url": "^2.1.0",
    "rollup-plugin-glslify": "^1.3.1",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-typescript2": "^0.34.1",
    "typescript": "^4.9.3"
  }
}
